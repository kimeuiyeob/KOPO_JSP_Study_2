package prac;

public class asd extends Thread {
	
	public void waitPlease() {
		
		Thread thr = new Thread();
		try {
			thr.sleep(1000);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
		
	}
	
	public static void main(String[] args) {
		asd s = new asd();
		for (int i = 1; i <= 3; i++) {
			System.out.println(i + "ì´ˆ");
			s.waitPlease();
		}
	}

}
