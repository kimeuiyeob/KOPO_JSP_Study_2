package Random;

import java.util.*;
public class AclassRandom {

   public static void main(String[] args) {
      
      
   }
   public List<String> Random(){
      String[] arr = {"강다영","김경연","김민지","김병훈","김은강","김의엽","김진우","남정현","노을","노한솔","박민정",
            "박진호","박현빈","박소연","신연주","우상욱"};
      List<Integer> a = new ArrayList<Integer>();
      List<String> b = new ArrayList<String>();


      while(true) {
         int tmp = (int)(Math.random()*16);
         if(!a.contains(tmp)){
            a.add(tmp);
            b.add(arr[tmp]);
         }
         if(a.size()==16) break;

      }

      return b;

   }   

}